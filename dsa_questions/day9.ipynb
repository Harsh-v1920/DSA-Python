    "#First alphabet of each string\n",
    "#Geeks for geeks\n",
    "\n",
    "\n",
    "class Solution:\n",
    "    def firstAlphabet(self, S):\n",
    "        # code here\n",
    "        lst=[i for i in S.split()]\n",
    "        t=\"\"\n",
    "        for i in lst:\n",
    "            t+=i[0]\n",
    "        return t\n",
    "            "
