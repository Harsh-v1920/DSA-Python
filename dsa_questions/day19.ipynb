    "#Geeks for geeks\n",
    "\n",
    "class Solution:\n",
    "    def Maximize(self, a, n): \n",
    "        # Complete the function\n",
    "        maxsum=0\n",
    "        a.sort()\n",
    "        for i in range(n):\n",
    "            maxsum+=arr[i]*i\n",
    "        return maxsum%(10**9+7)\n",
    "\n"
