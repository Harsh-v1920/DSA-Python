    "def find(arr,n,x):\n",
    "    # code here\n",
    "    ans=[]\n",
    "    for i in range(n):\n",
    "        if arr[i]==x:\n",
    "            ans.append(i)\n",
    "        if arr[i]>x:\n",
    "            break\n",
    "    if len(ans)>0:\n",
    "        t=[ans[0],ans[-1]]\n",
    "    else:\n",
    "        t=[-1,-1]\n",
    "    return t\n"
   ]
