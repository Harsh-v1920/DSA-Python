    "#Geeks for Geeks\n",
    "\n",
    "class Solution:\n",
    "\n",
    "\tdef count(self,arr, n, x):\n",
    "\t\t# code here\n",
    "\t\tcount=0\n",
    "\t\tfor i in arr:\n",
    "\t\t    if i==x:\n",
    "\t\t        count+=1\n",
    "\t\treturn count\n"
