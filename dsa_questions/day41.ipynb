    "def getNthFromLast(head,n):\n",
    "    #code here\n",
    "    count=0\n",
    "    i=head\n",
    "    while i.next!=None:\n",
    "        count+=1\n",
    "        i=i.next\n",
    "    # print(count)\n",
    "    if count+1<n:\n",
    "        return -1\n",
    "    j=0\n",
    "    \n",
    "    i=head\n",
    "    while j<=count-n:\n",
    "        i=i.next\n",
    "        j+=1\n",
    "    return (i.data)"
